<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CB_5e.Views.SplitPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Split the stack where?" />
            <Entry Keyboard="Numeric" Text="{Binding Split}" />
            <Button Text="Split" Clicked="Button_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>